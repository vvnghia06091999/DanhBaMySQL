<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.iuh.holo_app_chat">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".activities.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activities.danhba.TimDanhBaActivity"></activity>
        <activity android:name=".activities.danhba.PersonalPage" />
        <activity android:name=".activities.danhba.AddFriend" />
        <activity android:name=".activities.forgotpassword.CreatePassword" />
        <activity android:name=".activities.forgotpassword.InputToken" />
        <activity android:name=".activities.forgotpassword.ForgetPass" />
        <activity
            android:name=".activities.dashboard.DashboardActivity"
            android:label="@string/title_activity_dashboard" />
        <activity android:name=".activities.register.RegisterStep3Activity" />
        <activity android:name=".activities.register.RegisterStep2Activity" />
        <activity android:name=".activities.register.RegisterStep1Activity" />
        <activity
            android:name=".activities.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".activities.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>